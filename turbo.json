{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
      "build": {
          "dependsOn": ["^build"],
          "inputs": ["$TURBO_DEFAULT$", ".env*"],
          "outputs": ["dist/**"]
      },
      "lint": {},
      "test": {},
      "test:ui": {
          "cache": false,
          "persistent": true
      },
      "test:coverage": {},
      "type-check": {
          "dependsOn": ["^type-check"],
          "inputs": ["$TURBO_DEFAULT$", "tsconfig.json", "tsconfig.*.json"]
      },
      "dev": {
          "cache": false,
          "persistent": true
      }
  }
}
